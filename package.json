{
  "name": "react-disqus-thread",
  "version": "0.4.0",
  "description": "React Disqus thread component",
  "main": "build/DisqusThread.js",
  "module": "es/DisqusThread.js",
  "jsnext:main": "es/DisqusThread.js",
  "files": [
    "build",
    "es"
  ],
  "scripts": {
    "test": "rackt test --single-run --browsers Firefox",
    "start": "rackt server",
    "clean:build": "if [ -d build ]; then rm -rf build; fi",
    "clean:es": "if [ -d es ]; then rm -rf es; fi",
    "prebuild": "npm run clean:build && npm run clean:es",
    "build:cjs": "BABEL_ENV=cjs babel -d build --ignore DisqusThread-test.js lib",
    "build:es": "BABEL_ENV=es babel -d es --ignore DisqusThread-test.js lib",
    "build": "npm run build:cjs && npm run build:es"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mzabriskie/react-disqus-thread"
  },
  "keywords": [
    "disqus",
    "react",
    "react-component"
  ],
  "author": "Matt Zabriskie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mzabriskie/react-disqus-thread/issues"
  },
  "homepage": "https://github.com/mzabriskie/react-disqus-thread",
  "peerDependencies": {
    "react": "^15.0.1 || ^16.0.0",
    "react-dom": "^15.0.1 || ^16.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "rackt-cli": "^0.5.3",
    "react": "^15.0.1",
    "react-dom": "^15.0.1"
  },
  "dependencies": {
    "create-react-class": "^15.6.2",
    "prop-types": "^15.6.0"
  }
}
